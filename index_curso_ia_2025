  <script>
    // Animaciones de entrada
    document.addEventListener('DOMContentLoaded', function() {
      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            entry.target.style.animationDelay = Math.random() * 0.3 + 's';
            entry.target.classList.add('fade-in');
          }
        });
      });

      document.querySelectorAll('.week-card').forEach(card => {
        observer.observe(card);
      });

      // Scroll suave para el bot√≥n de inicio
      document.querySelector('.start-btn').addEventListener('click', function(e) {
        e.preventDefault();
        document.querySelector('#semanas').scrollIntoView({
          behavior: 'smooth'
        });
      });

      // Efecto de part√≠culas adicional en hover
      document.querySelectorAll('.week-card').forEach(card => {
        card.addEventListener('mouseenter', function() {
          this.style.transform = 'translateY(-8px) scale(1.02)';
        });
        
        card.addEventListener('mouseleave', function() {
          this.style.transform = 'translateY(0) scale(1)';
        });
      });
    });

    // Preloader para las p√°ginas de semanas
    function loadWeek(weekNumber) {
      const button = event.target.closest('.week-card');
      if (button) {
        button.style.opacity = '0.7';
        button.style.transform = 'scale(0.98)';
        
        setTimeout(() => {
          window.location.href = `semana${weekNumber}_Curso_IA2025.htm`;
        }, 200);
      }
    }
  </script>
</body>
</html>
